{
  "name": "ai-rules-builder",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev -- --mode local",
    "dev:e2e": "npm run astro dev -- --mode integration",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"src/**/*.{ts,tsx,astro}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx,astro}\"",
    "format": "prettier \"src/**/*.{astro,ts,tsx,css}\" --write",
    "format:check": "prettier \"src/**/*.{astro,ts,tsx,css}\" --check",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:codegen": "playwright codegen"
  },
  "dependencies": {
    "@astrojs/node": "9.1.3",
    "@astrojs/react": "4.2.1",
    "@hookform/resolvers": "5.0.1",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.49.3",
    "@tailwindcss/vite": "4.0.14",
    "@types/react-dom": "18.3.5",
    "astro": "^5.5.2",
    "clsx": "^2.1.1",
    "fflate": "^0.8.2",
    "lucide-react": "0.479.0",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "react-hook-form": "7.55.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "4.0.14",
    "typescript": "5.8.2",
    "zod": "3.24.2",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@playwright/test": "1.51.1",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/react": "18.3.18",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": "3.1.1",
    "astro-eslint-parser": "1.2.2",
    "dotenv": "16.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "1.3.1",
    "happy-dom": "17.4.4",
    "husky": "^9.0.11",
    "jsdom": "26.0.0",
    "lint-staged": "^15.2.2",
    "msw": "2.7.3",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "vitest": "3.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.37.0",
    "@tailwindcss/oxide-linux-x64-gnu": "4.0.14",
    "lightningcss-linux-x64-gnu": "1.29.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
